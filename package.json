{
  "name": "postfoo-server",
  "version": "1.0.0",
  "description": "PostFoo is project for tracking personal finance",
  "main": "dist/index.js",
  "scripts": {
    "build:gql": "copyfiles src/**/*.{graphql,mjml} dist -u 1",
    "prebuild": "npm run build:gql",
    "build": "tsc",
    "build:incr": "tsc -p tsconfig.json --incremental --tsBuildInfoFile dist/tsbuildinfo",
    "build:watch": "npm run build:incr -- --watch",
    "build:ondistchange": "onchange -k -i 'dist/**' -- npm run start:node",
    "build:ongqlchange": "onchange -k 'src/**/*.graphql' -- copyfiles '{{file}}' dist -u 1",
    "build:clean": "rimraf dist/*",
    "start": "npm run db:deploy && npm run start:node",
    "start:node": "node -- dist/index.js",
    "dev": "npm run build:gql && npm run build:incr && concurrently -kr npm:build:watch npm:build:ongqlchange npm:build:ondistchange",
    "db:init": "prisma migrate dev --name init",
    "db:reset": "prisma migrate reset",
    "db:format": "prisma format",
    "predb:migrate": "npm run db:format",
    "db:migrate": "prisma migrate dev",
    "db:migrate:dryrun": "prisma migrate dev --create-only --skip-seed",
    "db:deploy": "prisma migrate deploy",
    "postdb:deploy": "prisma generate",
    "gql:codegen": "graphql-codegen",
    "typecheck": "tsc -p tsconfig.json --noEmit --extendedDiagnostics",
    "lint": "eslint",
    "lint:fix": "npm run lint -- --fix",
    "test": "exit 1",
    "ts": "ts-node --transpile-only -r ./src/setup",
    "data:sync-funds": "npm run ts bin/data/sync-funds.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/umakantp/postfoo-server.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/umakantp/postfoo-server/issues"
  },
  "homepage": "https://github.com/umakantp/postfoo-server#readme",
  "devDependencies": {
    "@graphql-codegen/add": "^5.0.3",
    "@graphql-codegen/cli": "^5.0.3",
    "@graphql-codegen/typescript": "^4.1.2",
    "@graphql-codegen/typescript-resolvers": "^4.4.1",
    "@stylistic/eslint-plugin": "^2.13.0",
    "@stylistic/eslint-plugin-js": "^2.13.0",
    "@stylistic/eslint-plugin-ts": "^2.13.0",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/graphql-resolvers": "^0.3.2",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/lodash": "^4.17.14",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^22.10.7",
    "@types/otp-generator": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "concurrently": "^9.1.2",
    "copyfiles": "^2.4.1",
    "onchange": "^7.1.0",
    "prisma": "^6.2.1",
    "prisma-enums-generator": "^1.1.1",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.2",
    "@fastify/rate-limit": "^10.2.2",
    "@graphql-tools/load-files": "^7.0.1",
    "@graphql-tools/merge": "^9.0.17",
    "@graphql-tools/schema": "^10.0.16",
    "@oslojs/crypto": "^1.0.1",
    "@prisma/client": "^6.2.1",
    "@sentry/node": "^8.50.0",
    "@sentry/profiling-node": "^8.50.0",
    "chalk": "^5.4.1",
    "fastify": "^5.2.1",
    "google-auth-library": "^9.15.0",
    "googleapis": "^144.0.0",
    "graphql": "^16.10.0",
    "graphql-resolvers": "^0.4.2",
    "graphql-scalars": "^1.24.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mercurius": "^16.0.1",
    "module-alias": "^2.2.3",
    "moment": "^2.30.1",
    "otp-generator": "^4.0.1",
    "sendpulse-api": "^1.1.7",
    "tslib": "^2.8.1",
    "type-fest": "^4.32.0"
  },
  "_moduleAliases": {
    "src": "dist"
  }
}
