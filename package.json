{
  "name": "postfoo-server",
  "version": "1.0.0",
  "description": "Post Foo is project for tracking personal finance",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:incr": "tsc -p tsconfig.json --incremental --tsBuildInfoFile dist/tsbuildinfo",
    "build:watch": "npm run build:incr -- --watch",
    "build:onchange": "onchange -k -i 'dist/**' -- npm run start:node",
    "build:clear": "rimraf dist/*",
    "start:node": "node -- dist/index.js",
    "dev": "npm run build:incr && concurrently -kr npm:build:watch npm:build:onchange",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/umakantp/postfoo-server.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/umakantp/postfoo-server/issues"
  },
  "homepage": "https://github.com/umakantp/postfoo-server#readme",
  "devDependencies": {
    "@stylistic/eslint-plugin": "^2.12.1",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "concurrently": "^9.1.1",
    "onchange": "^7.1.0",
    "prisma": "^6.1.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.1",
    "@prisma/client": "^6.1.0",
    "chalk": "^5.4.1",
    "fastify": "^5.2.0",
    "mercurius": "^16.0.0",
    "module-alias": "^2.2.3",
    "tslib": "^2.8.1"
  },
  "_moduleAliases": {
    "src": "dist/src"
  }
}
